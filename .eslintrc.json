{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true,
        "mocha": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "import/no-unresolved": "off",
        "no-var": 0,
        "camelcase": "off",
        "no-console": "warn",
        "vars-on-top": "off",
        "spaced-comment": "off",
        "consistent-return": "off",
        "func-names": "off",
        "object-shorthand": "off",
        "no-process-exit": "off",
        "no-param-reassign": "off",
        "no-return-await": "off",
        "no-restricted-globals": "off",
        "no-shadow": "off",
        "prefer-const": "off",
        "new-cap": "off",
        "strict": "error",
        "comma-dangle": "off",
        "lines-around-directive": "off",
        "no-underscore-dangle": "off",
        "class-methods-use-this": "off",
        "no-use-before-define": "off",
        "no-unused-vars": "off",
        "semi": "off",
        "arrow-parens": "off",
        "no-undef": "off",
        "allowIndentationTabs": 0,
        "no-tabs": "off",
        "indent": "off",
        "eqeqeq": "off"
    }
}